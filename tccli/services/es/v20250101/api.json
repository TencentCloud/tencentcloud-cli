    "WebSearch": {
      "document": "WebSearch API 是一个网页搜索服务，支持多种搜索引擎，可以获取网页的标题、URL、摘要和正文内容。",
      "input": "WebSearchRequest",
      "name": "网页搜索",
      "output": "WebSearchResponse",
      "status": "online"
    }
