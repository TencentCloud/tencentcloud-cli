          "member": "string",
          "name": "HostUuid",
          "output_required": false,
          "type": "string",
          "value_allowed_null": true
        },
        {
          "disabled": false,
          "document": "实例名称数组\n注意：此字段可能返回 null，表示取不到有效值。",
          "example": "['ins-xxxxxxxx']",
          "member": "string",
          "name": "InstancesInfo",
          "output_required": false,
          "type": "list",
          "value_allowed_null": true
        }
      ],
      "usage": "out"
    },
