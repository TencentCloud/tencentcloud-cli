{
  "actions": {
    "CompleteApproval": {
      "document": "执行审批",
      "input": "CompleteApprovalRequest",
      "name": "执行审批",
      "output": "CompleteApprovalResponse",
      "status": "online"
    },
    "CreateRoleUser": {
      "document": "创建人员",
      "input": "CreateRoleUserRequest",
      "name": "创建人员",
      "output": "CreateRoleUserResponse",
      "status": "online"
    },
    "DeleteRoleUser": {
      "document": "删除自定义用户",
      "input": "DeleteRoleUserRequest",
      "name": "删除自定义角色的用户",
      "output": "DeleteRoleUserResponse",
      "status": "online"
    },
    "PutEvent": {
      "document": "推送事件数据",
      "input": "PutEventRequest",
      "name": "推送事件",
      "output": "PutEventResponse",
      "status": "online"
    },
    "PutMessage": {
      "document": "推送事件数据",
      "input": "PutMessageRequest",
      "name": "推送消息",
      "output": "PutMessageResponse",
      "status": "online"
    }
  },
  "metadata": {
    "apiVersion": "2025-02-17",
    "api_brief": "事件中心可支持“拖拉拽”方式生成页面表单，支持丰富的审批能力，支持API对接交互，支持多样的流程逻辑配置等。可以低代码方式实现较复杂业务流程的搭建、审批、数据流转等。",
    "serviceNameCN": "事件中心",
    "serviceShortName": "evt"
  },
  "objects": {
    "CompleteApprovalRequest": {
      "document": "CompleteApproval请求参数结构体",
      "members": [
        {
          "disabled": false,
          "document": "<p>审批单号</p>",
          "example": "A202500000000000000000",
          "member": "string",
          "name": "ApprovalId",
          "required": true,
          "type": "string"
        },
        {
          "disabled": false,
          "document": "<p>审批节点</p>",
          "example": "AN202500000000000000000",
          "member": "string",
          "name": "NodeId",
          "required": true,
          "type": "string"
        },
        {
          "disabled": false,
          "document": "<p>审批结果，1通过2拒绝</p>",
          "example": "1",
          "member": "uint64",
          "name": "Result",
          "required": true,
          "type": "int"
        },
        {
          "disabled": false,
          "document": "<p>审批意见</p>",
          "example": "通过",
          "member": "string",
          "name": "Opinion",
          "required": false,
          "type": "string"
        },
        {
          "disabled": false,
          "document": "<p>审批人的身份认证Token，通过自定义角色体系回调接口分发</p><p>token信息</p>",
          "example": "zFFJ3RrUVc6fBMeekMBFnhd5pfvlk4W",
          "member": "string",
          "name": "UserToken",
          "required": false,
          "type": "string"
        }
      ],
      "type": "object"
    },
    "CompleteApprovalResponse": {
      "document": "CompleteApproval返回参数结构体",
      "members": [
        {
          "document": "唯一请求 ID，由服务端生成，每次请求都会返回（若请求因其他原因未能抵达服务端，则该次请求不会获得 RequestId）。定位问题时需要提供该次请求的 RequestId。",
          "member": "string",
          "name": "RequestId",
          "type": "string"
        }
      ],
      "type": "object"
    },
