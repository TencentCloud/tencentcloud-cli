          "name": "L4RankExpression",
          "required": false,
          "type": "string"
        },
        {
          "default": "",
          "document": "高级排序参数，具体参见高级排序说明",
          "member": "string",
          "name": "MatchValue",
          "required": false,
          "type": "string"
        },
        {
          "default": "",
          "document": "经度信息",
          "member": "float",
          "name": "Longitude",
          "required": false,
          "type": "float"
        },
        {
          "default": "",
          "document": "纬度信息",
          "member": "float",
          "name": "Latitude",
          "required": false,
          "type": "float"
        },
        {
          "default": "",
          "document": "分类过滤并集",
          "member": "string",
          "name": "MultiFilter",
          "required": false,
          "type": "list"
        }
      ],
      "type": "object"
    },
