          "member": "uint64",
          "name": "NumPerPage",
          "required": false,
          "type": "int"
        },
        {
          "default": "",
          "document": "当前检索号，用于定位问题，建议指定并且全局唯一",
          "member": "string",
          "name": "SearchId",
          "required": false,
          "type": "string"
        },
        {
          "default": "",
