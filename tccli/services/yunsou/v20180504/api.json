          "member": "string",
          "name": "OpType",
          "required": true,
          "type": "string"
        },
        {
          "default": "",
          "document": "数据编码类型",
          "member": "string",
          "name": "Encoding",
          "required": true,
          "type": "string"
        },
        {
          "default": "",
          "document": "数据",
          "member": "string",
          "name": "Contents",
          "required": true,
          "type": "string"
        },
        {
          "default": "",
          "document": "应用Id",
          "member": "uint64",
          "name": "ResourceId",
          "required": true,
          "type": "int"
        }
      ],
      "type": "object"
    },
    "DataManipulationResponse": {
      "document": "DataManipulation返回参数结构体",
