{
  "actions": {
    "CloudMateAgent": {
      "document": "发起智能诊断对话\n注意：使用该API时，请务必设置请求域名（Endpoint）为 cloudmate.ai.tencentcloudapi.com",
      "input": "CloudMateAgentRequest",
      "name": "发起智能诊断对话",
      "output": "CloudMateAgentResponse",
      "status": "online"
    }
  },
  "metadata": {
    "apiVersion": "2025-10-30",
    "api_brief": "可通过 API 发起异常诊断，对接各类运维系统及工具，实现自动化异常问题排查",
    "serviceNameCN": "Cloud Mate",
    "serviceShortName": "cloudmate"
  },
  "objects": {
