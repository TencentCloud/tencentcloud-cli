    "CloudMateAgentRequest": {
      "document": "CloudMateAgent请求参数结构体",
      "members": [
        {
          "disabled": false,
          "document": "空间 ID",
          "example": "fa5234dc-0663-4447-ab71-ed2776fc1137",
          "member": "string",
          "name": "WorkspaceId",
          "required": true,
          "type": "string"
        },
        {
          "disabled": false,
          "document": "用户提问内容",
          "example": "帮我分析下最近30分钟的系统错误",
          "member": "string",
          "name": "Message",
          "required": true,
          "type": "string"
        },
        {
          "disabled": false,
          "document": "场景 ID",
          "example": "01248bc1-c11f-4683-a960-11c534572424",
          "member": "string",
          "name": "ScenarioId",
          "required": false,
          "type": "string"
        },
        {
          "disabled": false,
          "document": "会话 ID\n\n- 填写上一次接口调用返回的会话 ID（SessionId），可在原有会话基础上继续对话\n- 未填写会话 ID 时，创建新会话",
          "example": "ff9012c0-90d8-4bc2-9545-31fb5962bfb7",
          "member": "string",
          "name": "SessionId",
          "required": false,
          "type": "string"
