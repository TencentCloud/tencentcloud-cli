      "document": "智能诊断结果内容结构",
      "members": [
        {
          "disabled": false,
          "document": "角色，可选值：\n - user\n - model_thinking\n - model_output\n - knowledge",
          "example": "user",
          "member": "string",
          "name": "Role",
          "output_required": false,
          "required": false,
          "type": "string",
          "value_allowed_null": false
        },
        {
          "disabled": false,
          "document": "内容分片\n注意：此字段可能返回 null，表示取不到有效值。",
          "example": "无",
          "member": "ChatEventContentPart",
          "name": "Parts",
          "output_required": false,
          "required": false,
          "type": "list",
          "value_allowed_null": true
        }
