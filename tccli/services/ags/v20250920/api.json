{
  "actions": {
    "AcquireSandboxInstanceToken": {
      "document": "获取访问沙箱工具时所需要使用的访问Token，创建沙箱实例后需调用此接口获取沙箱实例访问Token。\n此Token可用于调用代码沙箱实例执行代码，或浏览器沙箱实例进行浏览器操作等。",
      "input": "AcquireSandboxInstanceTokenRequest",
      "name": "获取沙箱实例的访问Token",
      "output": "AcquireSandboxInstanceTokenResponse",
      "status": "online"
    },
    "CreateAPIKey": {
      "document": "创建新的API密钥，用于调用Agent Sandbox接口。相较于腾讯云Secret ID Secret Key支持调用所有接口使用，仅有部分接口支持使用API密钥调用。",
      "input": "CreateAPIKeyRequest",
      "name": "创建新的API密钥",
      "output": "CreateAPIKeyResponse",
      "status": "online"
    },
    "CreateSandboxTool": {
      "document": "创建沙箱工具",
      "input": "CreateSandboxToolRequest",
      "name": "创建沙箱工具",
      "output": "CreateSandboxToolResponse",
      "status": "online"
    },
    "DeleteAPIKey": {
      "document": "删除API密钥。注意区别于腾讯云Secret ID Secret Key，本接口删除的是Agent Sandbox专用API key。",
      "input": "DeleteAPIKeyRequest",
      "name": "删除API密钥",
      "output": "DeleteAPIKeyResponse",
      "status": "online"
    },
    "DeleteSandboxTool": {
      "document": "删除沙箱工具",
      "input": "DeleteSandboxToolRequest",
      "name": "删除沙箱工具",
      "output": "DeleteSandboxToolResponse",
      "status": "online"
    },
    "DescribeAPIKeyList": {
      "document": "获取API密钥列表，包含API密钥简略信息，包含名称、创建时间等。",
      "input": "DescribeAPIKeyListRequest",
      "name": "获取API密钥列表",
      "output": "DescribeAPIKeyListResponse",
      "status": "online"
    },
    "DescribeSandboxInstanceList": {
      "document": "查询沙箱实例列表",
      "input": "DescribeSandboxInstanceListRequest",
      "name": "查询沙箱实例列表",
      "output": "DescribeSandboxInstanceListResponse",
      "status": "online"
    },
    "DescribeSandboxToolList": {
      "document": "查询沙箱工具列表",
      "input": "DescribeSandboxToolListRequest",
      "name": "查询沙箱工具列表",
      "output": "DescribeSandboxToolListResponse",
      "status": "online"
    },
    "StartSandboxInstance": {
      "document": "启动沙箱实例",
      "input": "StartSandboxInstanceRequest",
      "name": "启动沙箱实例",
      "output": "StartSandboxInstanceResponse",
      "status": "online"
    },
    "StopSandboxInstance": {
      "document": "停止沙箱实例",
      "input": "StopSandboxInstanceRequest",
      "name": "停止沙箱实例",
      "output": "StopSandboxInstanceResponse",
      "status": "online"
    },
    "UpdateSandboxInstance": {
      "document": "更新沙箱实例",
      "input": "UpdateSandboxInstanceRequest",
      "name": "更新沙箱实例",
      "output": "UpdateSandboxInstanceResponse",
      "status": "online"
    },
    "UpdateSandboxTool": {
      "document": "更新沙箱工具",
      "input": "UpdateSandboxToolRequest",
      "name": "更新沙箱工具",
      "output": "UpdateSandboxToolResponse",
      "status": "online"
    }
  },
