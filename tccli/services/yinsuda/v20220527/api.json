{
  "actions": {
    "BatchDescribeKTVMusicDetails": {
      "document": "批量获取歌曲详细信息，包括：歌词下载链接、播放凭证、音高数据下载链接信息等。",
      "input": "BatchDescribeKTVMusicDetailsRequest",
      "name": "批量获取歌曲详情",
      "output": "BatchDescribeKTVMusicDetailsResponse"
    },
    "CreateKTVRobot": {
      "document": "创建机器人，支持进入 RTC 房间，播放曲库歌曲。",
      "input": "CreateKTVRobotRequest",
      "name": "创建机器人",
      "output": "CreateKTVRobotResponse"
    },
    "DescribeKTVMatchMusics": {
      "document": "根据输入的规则匹配曲库中的歌曲。",
      "input": "DescribeKTVMatchMusicsRequest",
      "name": "匹配歌曲",
      "output": "DescribeKTVMatchMusicsResponse"
    },
    "DescribeKTVMusicsByTag": {
      "document": "通过标签过滤歌曲列表。",
      "input": "DescribeKTVMusicsByTagRequest",
      "name": "获取标签歌曲",
      "output": "DescribeKTVMusicsByTagResponse"
    },
    "DescribeKTVPlaylistDetail": {
      "document": "根据歌单 Id 获取歌单详情。",
      "input": "DescribeKTVPlaylistDetailRequest",
      "name": "获取歌单详情",
      "output": "DescribeKTVPlaylistDetailResponse"
    },
    "DescribeKTVPlaylists": {
      "document": "获取歌单列表。",
      "input": "DescribeKTVPlaylistsRequest",
      "name": "获取推荐歌单列表",
      "output": "DescribeKTVPlaylistsResponse"
    },
    "DescribeKTVRobots": {
      "document": "获取机器人列表，支持 Id、状态等过滤条件。",
      "input": "DescribeKTVRobotsRequest",
      "name": "获取机器人信息",
      "output": "DescribeKTVRobotsResponse"
    },
    "DescribeKTVSuggestions": {
      "document": "根据关键词获取联想词列表。",
      "input": "DescribeKTVSuggestionsRequest",
      "name": "获取联想词",
      "output": "DescribeKTVSuggestionsResponse"
    },
    "DescribeKTVTags": {
      "document": "获取标签分组及分组下的标签列表信息。",
      "input": "DescribeKTVTagsRequest",
      "name": "获取标签列表",
      "output": "DescribeKTVTagsResponse"
    },
    "DestroyKTVRobot": {
      "document": "销毁机器人，机器人退出 RTC 房间。",
      "input": "DestroyKTVRobotRequest",
      "name": "销毁机器人",
      "output": "DestroyKTVRobotResponse"
    },
    "SearchKTVMusics": {
      "document": "根据关键词搜索歌曲，返回相关歌曲列表。",
      "input": "SearchKTVMusicsRequest",
      "name": "搜索歌曲",
      "output": "SearchKTVMusicsResponse"
    },
    "SyncKTVRobotCommand": {
      "document": "下发操作机器人指令，支持播放、暂停、恢复、歌单设置等操作指令，实现对机器人行为的控制。",
      "input": "SyncKTVRobotCommandRequest",
      "name": "同步机器人指令",
      "output": "SyncKTVRobotCommandResponse"
    }
  },
  "metadata": {
    "apiVersion": "2022-05-27",
    "api_brief": "音速达直播音乐版权引擎服务基于是基于腾讯音乐娱乐集团（TME）海量线上直播音乐版权资源，结合腾讯云存储、内容加速分发等基础能力，搭配演唱评分、声音美化等智能音乐功能，为解决直播语聊场景中的正版音乐应用设计的 PaaS 产品。您可以前往API概览了解详情。",
    "serviceNameCN": "音速达直播音乐版权引擎",
    "serviceShortName": "yinsuda"
  },
