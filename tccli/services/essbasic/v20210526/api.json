{
  "actions": {
    "CreateConsoleLoginUrl": {
      "document": "此接口（CreateConsoleLoginUrl）用于创建电子签控制台登录链接。若企业未激活，调用同步企业信息、同步经办人信息\n",
      "input": "CreateConsoleLoginUrlRequest",
      "name": "生成控制台链接",
      "output": "CreateConsoleLoginUrlResponse"
    },
    "CreateFlowsByTemplates": {
      "document": "接口（CreateFlowsByTemplates）用于使用多个模板批量创建流程",
      "input": "CreateFlowsByTemplatesRequest",
      "name": "使用多个模板批量创建流程",
      "output": "CreateFlowsByTemplatesResponse"
    },
    "CreateSignUrls": {
      "document": "根据流程Id批量创建签署参与者签署H5链接",
      "input": "CreateSignUrlsRequest",
      "name": "获取跳转小程序签署链接",
      "output": "CreateSignUrlsResponse"
    },
    "DescribeTemplates": {
      "document": "通过此接口（DescribeTemplates）查询该企业在电子签渠道版中配置的有效模板列表",
      "input": "DescribeTemplatesRequest",
      "name": "查询模板信息列表",
      "output": "DescribeTemplatesResponse"
    },
    "DescribeUsage": {
      "document": "此接口（DescribeUsage）用于获取渠道所有合作企业流量消耗情况。\n 注: 此接口每日限频2次，若要扩大限制次数,请提前与客服经理或邮件至e-contract@tencent.com进行联系。",
      "input": "DescribeUsageRequest",
      "name": "渠道用量查询",
      "output": "DescribeUsageResponse"
    },
    "PrepareFlows": {
      "document": "该接口 (PrepareFlows) 用于创建待发起文件",
      "input": "PrepareFlowsRequest",
      "name": "准备待发起文件",
      "output": "PrepareFlowsResponse"
    },
    "SyncProxyOrganization": {
      "document": "此接口（SyncProxyOrganization）用于同步渠道侧企业信息",
      "input": "SyncProxyOrganizationRequest",
      "name": "同步企业信息",
      "output": "SyncProxyOrganizationResponse"
    },
    "SyncProxyOrganizationOperators": {
      "document": "此接口（SyncProxyOrganizationOperators）用于同步渠道合作企业经办人列表",
      "input": "SyncProxyOrganizationOperatorsRequest",
      "name": "同步企业经办人列表",
      "output": "SyncProxyOrganizationOperatorsResponse"
    }
  },
  "metadata": {
    "apiVersion": "2021-05-26",
    "api_brief": "使用API接口发起电子签名流程, 对流程进行管理, 电子印章管理,数字证书管理, 角色组织架构机构帐号管理,文档管理, 日志及统计等功能",
    "serviceNameCN": "腾讯电子签（基础版）",
    "serviceShortName": "essbasic"
  },
