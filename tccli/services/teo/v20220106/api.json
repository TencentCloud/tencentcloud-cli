          "member": "string",
          "name": "ZoneId",
          "required": true,
          "type": "string"
        },
        {
          "default": "",
          "document": "类型，当前支持的类型：\n- purge_url：URL\n- purge_prefix：前缀\n- purge_host：Hostname\n- purge_all：全部缓存",
          "example": "purge_url",
          "member": "string",
          "name": "Type",
          "required": true,
          "type": "string"
        },
        {
          "default": "",
